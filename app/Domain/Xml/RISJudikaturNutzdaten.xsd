                    <xs:element ref="literaliste"/>
                    <xs:element ref="subliteraliste"/>
                    <xs:element ref="strichliste"/>
                    <xs:element ref="schluss"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="ebene" default="1">
                <xs:simpleType>
                    <xs:restriction base="xs:nonNegativeInteger">
                        <xs:enumeration value="1">
                            <xs:annotation>
                                <xs:documentation>52_Ziffer_e1</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="2">
                            <xs:annotation>
                                <xs:documentation>52_Ziffer_e2 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="3">
                            <xs:annotation>
                                <xs:documentation>52_Ziffer_e3 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="4">
                            <xs:annotation>
                                <xs:documentation>52_Ziffer_e4 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="5">
                            <xs:annotation>
                                <xs:documentation>52_Ziffer_e5 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="literaliste">
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="listelem"/>
                    <xs:element ref="subliteraliste"/>
                    <xs:element ref="strichliste"/>
                    <xs:element ref="schluss"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="ebene" default="2">
                <xs:simpleType>
                    <xs:restriction base="xs:nonNegativeInteger">
                        <xs:enumeration value="1">
                            <xs:annotation>
                                <xs:documentation>53_Litera_e1 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="2">
                            <xs:annotation>
                                <xs:documentation>53_Litera_e2</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="3">
                            <xs:annotation>
                                <xs:documentation>53_Litera_e3</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="4">
                            <xs:annotation>
                                <xs:documentation>53_Litera_e4</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="5">
                            <xs:annotation>
                                <xs:documentation>53_Litera_e5</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="subliteraliste">
