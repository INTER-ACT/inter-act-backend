        </xs:annotation>
    </xs:element>
    <!-- ////////////////////////////////////////////////////////////////// -->
    <xs:element name="src" type="xs:string">
        <xs:annotation>
            <xs:documentation>Referenz (Pfad + Dateiname) auf extern gehaltenes File. Nur vorhanden, wenn nicht Base64 eingebettet.</xs:documentation>
        </xs:annotation>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // Listen/Aufzählungen                                                       -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="liste">
        <xs:annotation>
            <xs:documentation>Aufzählungen</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="false">
            <xs:choice maxOccurs="unbounded">
                <xs:element ref="ziffernliste"/>
                <xs:element ref="literaliste"/>
                <xs:element ref="subliteraliste"/>
                <xs:element ref="strichliste"/>
                <xs:element ref="betragliste"/>
                <xs:element ref="betraglistetgue"/>
                <xs:element ref="erlliste"/>
                <xs:element ref="schluss"/>
            </xs:choice>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="listelem">
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:element ref="symbol"/>
                <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
