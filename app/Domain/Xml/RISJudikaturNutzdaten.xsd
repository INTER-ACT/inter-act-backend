                    <xs:element ref="schluss"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="ebene" default="3">
                <xs:simpleType>
                    <xs:restriction base="xs:nonNegativeInteger">
                        <xs:enumeration value="1">
                            <xs:annotation>
                                <xs:documentation>54_Sublitera_e1 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="2">
                            <xs:annotation>
                                <xs:documentation>54_Sublitera_e2 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="3">
                            <xs:annotation>
                                <xs:documentation>54_Sublitera_e3</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="4">
                            <xs:annotation>
                                <xs:documentation>54_Sublitera_e4</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="5">
                            <xs:annotation>
                                <xs:documentation>54_Sublitera_e5</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="strichliste">
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="listelem"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="ebene" default="4">
                <xs:simpleType>
                    <xs:restriction base="xs:nonNegativeInteger">
                        <xs:enumeration value="1">
                            <xs:annotation>
                                <xs:documentation>54a_Strich_e1 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="2">
                            <xs:annotation>
                                <xs:documentation>54a_Strich_e2 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="3">
                            <xs:annotation>
                                <xs:documentation>54a_Strich_e3 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="4">
                            <xs:annotation>
                                <xs:documentation>54a_Strich_e4</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="5">
                            <xs:annotation>
                                <xs:documentation>54a_Strich_e5 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="6">
                            <xs:annotation>
                                <xs:documentation>54a_Strich_e6 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="7">
                            <xs:annotation>
                                <xs:documentation>54a_Strich_e7 (intern)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="betragliste">
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="listelem"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="typ">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="ziffer">
                            <xs:annotation>
                                <xs:documentation>52_Ziffer_mit_Betrag</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="litera">
                            <xs:annotation>
                                <xs:documentation>53_Litera_mit_Betrag</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="sublitera">
                            <xs:annotation>
                                <xs:documentation>54_Sublitera_mit_Betrag</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="betraglistetgue">
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="listelem"/>
                </xs:choice>
            </xs:sequence>
            <xs:attribute name="typ">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="ziffer">
                            <xs:annotation>
                                <xs:documentation>52aT_Ziffer_mit_Betrag_TGUE</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="litera">
                            <xs:annotation>
                                <xs:documentation>53aT_Litera_mit_Betrag_TGUE</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="sublitera">
                            <xs:annotation>
                                <xs:documentation>54aT_Sublitera_mit_Betrag_TGUE</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="erlliste">
        <xs:annotation>
            <xs:documentation>85_ErlAufzaehlg</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:choice minOccurs="0" maxOccurs="unbounded">
                    <xs:element ref="listelem"/>
                </xs:choice>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="schluss">
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
            <xs:attribute name="typ">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Abs">
                            <xs:annotation>
                                <xs:documentation>55_SchlussteilAbs</xs:documentation>
                            </xs:annotation>
                            <!-- A.H., 31. Jän. 2008: das entspricht Ebene 0 -->
                        </xs:enumeration>
                        <xs:enumeration value="e1">
                            <xs:annotation>
                                <xs:documentation>57_Schlussteil_e1</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="Ziff">
                            <xs:annotation>
                                <xs:documentation>56_SchlussteilZiff</xs:documentation>
                            </xs:annotation>
                            <!-- A.H., 31. Jän. 2008: das entspricht Ebene 2 -->
                        </xs:enumeration>
                        <xs:enumeration value="Lit">
                            <xs:annotation>
                                <xs:documentation>57_SchlussteilLit</xs:documentation>
                            </xs:annotation>
                            <!-- A.H., 31. Jän. 2008: das entspricht Ebene 3 -->
                        </xs:enumeration>
                        <xs:enumeration value="e4">
                            <xs:annotation>
                                <xs:documentation>57_Schlussteil_e4</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="e5">
                            <xs:annotation>
                                <xs:documentation>57_Schlussteil_e5</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="ct" type="xs:string" use="optional"/>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // Inhaltsverzeichnis                                                 -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="inhaltsvz">
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
            <xs:attribute name="typ">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="ueberschrift">
                            <xs:annotation>
                                <xs:documentation>30_InhaltUeberschrift</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="spalte">
                            <xs:annotation>
                                <xs:documentation>31_InhaltSpalte</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="eintrag">
                            <xs:annotation>
                                <xs:documentation>32_InhaltEintrag</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="halign"/>
            <xs:attribute name="ct" type="xs:string" use="optional"/>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // Unterschriften                                                   -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="unterschrift">
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
            <xs:attribute name="typ" use="required">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="links">
                            <xs:annotation>
                                <xs:documentation>68_UnterschrL</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="mitte">
                            <xs:annotation>
                                <xs:documentation>69_UnterschrM</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="rechts">
                            <xs:annotation>
                                <xs:documentation>Optional: frei</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="halign"/>
            <xs:attribute name="ct" type="xs:string" use="optional"/>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // Zeichenformate (-vorlagen)                                                -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:group name="zeichenformate">
        <xs:choice>
            <xs:element ref="error"/>
            <xs:element ref="gldsym"/>
            <xs:element ref="n"/>
            <xs:element ref="b"/>
            <xs:element ref="i"/>
            <xs:element ref="u"/>
            <xs:element ref="gs"/>
            <xs:element ref="super"/>
            <xs:element ref="sub"/>
            <xs:element ref="s"/>
            <!-- A.H., 31. Jän. 2008: Da in manchen DSK-Dokumenten auch Durchstreichungen vorkommen können. -->
        </xs:choice>
    </xs:group>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="error">
        <xs:annotation>
            <xs:documentation>990_Fehler</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="gldsym">
        <xs:annotation>
            <xs:documentation>991_GldSymbol</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="n">
        <xs:annotation>
            <xs:documentation>992_Normal</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="b">
        <xs:annotation>
            <xs:documentation>993_Fett bzw. 999_Fett_und_Kursiv</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="i">
        <xs:annotation>
            <xs:documentation>994_Kursiv bzw. 999_Fett_und_Kursiv</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="u">
        <xs:annotation>
            <xs:documentation>995_Unterstrichen</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="gs">
        <xs:annotation>
            <xs:documentation>996_Gesperrt</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
            <xs:attribute name="sperrweite" type="xs:string" use="optional"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="super">
        <xs:annotation>
            <xs:documentation>997_Hoch (siehe CSS: vertical-align)</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="sub">
        <xs:annotation>
            <xs:documentation>998_Tief (siehe CSS: vertical-align)</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="s">
        <xs:annotation>
            <xs:documentation>Durchgestrichen</xs:documentation>
        </xs:annotation>
        <!-- A.H., 31. Jän. 2008: Da in manchen DSK-Dokumenten auch Durchstreichungen vorkommen können. -->
        <xs:complexType mixed="true">
            <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
        </xs:complexType>
    </xs:element>
    <xs:element name="uchar">
        <xs:annotation>
            <xs:documentation>Legistik 2005: Sonderzeichen</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="font" type="xs:string"/>
            <xs:attribute name="code" type="xs:string"/>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // Felder                                                               -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="feld">
        <xs:annotation>
            <xs:documentation>Enthält das Resultat; darf mit Zeichenformaten formatiert sein. Hat sonst das Format des Absatzes.</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:group ref="zeichenformate" minOccurs="0" maxOccurs="unbounded"/>
            <xs:attribute name="code" type="xs:string">
                <xs:annotation>
                    <xs:documentation>Code aus MS Word</xs:documentation>
                </xs:annotation>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // Hyperlink                                                           -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="link">
        <xs:annotation>
            <xs:documentation>Stellt einen Hyperlink dar.</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:group ref="zeichenformate" minOccurs="0" maxOccurs="unbounded"/>
                <xs:element ref="nbsp"/>
                <xs:element ref="bdash"/>
                <xs:element ref="gdash"/>
                <xs:element ref="kidea"/>
                <xs:element ref="lidea"/>
                <xs:element ref="euro"/>
                <xs:element ref="lt"/>
                <xs:element ref="gt"/>
                <xs:element ref="amp"/>
                <xs:element ref="bdquo"/>
                <xs:element ref="ldquo"/>
                <xs:element ref="em"/>
                <xs:element ref="en"/>
                <xs:element ref="em14"/>
                <xs:element ref="tm"/>
                <xs:element ref="dots"/>
                <xs:element ref="rdquo"/>
                <xs:element ref="sbquo"/>
                <xs:element ref="rsquo"/>
                <xs:element ref="lsquo"/>
                <xs:element ref="uchar"/>
            </xs:choice>
            <xs:attribute name="href" type="xs:string"/>
            <xs:attribute name="target" type="xs:string"/>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // Was ist innerhalb eines Absatzes erlaubt   -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:group name="innerhalbAbsatz">
        <xs:annotation>
            <xs:documentation>Was ist innerhalb eines Absatzes erlaubt?</xs:documentation>
        </xs:annotation>
        <xs:choice>
            <xs:group ref="zeichenformate" minOccurs="0" maxOccurs="unbounded"/>
            <xs:element ref="br"/>
            <xs:element ref="tab"/>
            <xs:element ref="fnzeichen"/>
            <xs:element ref="feld"/>
            <xs:element ref="link"/>
            <xs:element ref="nbsp"/>
            <xs:element ref="bdash"/>
            <xs:element ref="gdash"/>
            <xs:element ref="kidea"/>
            <xs:element ref="lidea"/>
            <xs:element ref="euro"/>
            <xs:element ref="lt"/>
            <xs:element ref="gt"/>
            <xs:element ref="amp"/>
            <xs:element ref="bdquo"/>
            <xs:element ref="ldquo"/>
            <xs:element ref="em"/>
            <xs:element ref="en"/>
            <xs:element ref="em14"/>
            <xs:element ref="tm"/>
            <xs:element ref="dots"/>
            <xs:element ref="rdquo"/>
            <xs:element ref="sbquo"/>
            <xs:element ref="rsquo"/>
            <xs:element ref="lsquo"/>
            <xs:element ref="binary"/>
            <!--xs:group ref="verweise"/-->
            <xs:element ref="aw"/>
            <xs:element ref="wechsel"/>
            <xs:element ref="uchar"/>
        </xs:choice>
    </xs:group>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="br">
        <xs:annotation>
            <xs:documentation>Manueller Zeilenumbruch (^11 bzw. ^l)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="class" type="xs:string"/>
            <xs:attribute name="style" type="xs:string"/>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="aw">
        <xs:annotation>
            <xs:documentation>Abschnittswechsel: ^b</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="tab">
        <xs:annotation>
            <xs:documentation>Tabulator</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="align">
                <xs:annotation>
                    <xs:documentation>Ausrichtung des Tabulators (l, z, r, d, vl, li)</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="l">
                            <xs:annotation>
                                <xs:documentation>links</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="z">
                            <xs:annotation>
                                <xs:documentation>zentriert</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="r">
                            <xs:annotation>
                                <xs:documentation>rechts</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="d">
                            <xs:annotation>
                                <xs:documentation>dezimal</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="vl">
                            <xs:annotation>
                                <xs:documentation>vertikale Linie</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="li">
                            <xs:annotation>
                                <xs:documentation>Liste (word intern?)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="position" type="xs:string"/>
            <xs:attribute name="leader">
                <xs:annotation>
                    <xs:documentation>Führungslinie durch Punkte, Striche,...</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="o">
                            <xs:annotation>
                                <xs:documentation>Ohne</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="p">
                            <xs:annotation>
                                <xs:documentation>Punkte</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="mp">
                            <xs:annotation>
                                <xs:documentation>Punkte in der Mitte des Zeilenhöhe</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="s">
                            <xs:annotation>
                                <xs:documentation>Striche</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="u">
                            <xs:annotation>
                                <xs:documentation>Unterstriche (underscore)</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
            <!-- empty -->
            <!-- absolut  in pt-->
        </xs:complexType>
    </xs:element>
    <xs:element name="nbsp">
        <xs:annotation>
            <xs:documentation>Geschütztes Leerzeichen</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Geschützes Leerzeichen ^s, ^0160</xs:documentation>
            </xs:annotation>
        </xs:complexType>
    </xs:element>
    <xs:element name="gdash">
        <xs:annotation>
            <xs:documentation>Geschützter Trennstrich</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Geschützter Trennstrich ^~, ^030</xs:documentation>
            </xs:annotation>
        </xs:complexType>
    </xs:element>
    <xs:element name="bdash">
        <xs:annotation>
            <xs:documentation>Bedingter Trennstrich</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Bedingter Trennstrich ^-, ^031</xs:documentation>
            </xs:annotation>
        </xs:complexType>
    </xs:element>
    <xs:element name="kidea">
        <xs:annotation>
            <xs:documentation>Kurzer Gedankenstrich</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Kurzer Gedankenstrich ^=</xs:documentation>
            </xs:annotation>
        </xs:complexType>
    </xs:element>
    <xs:element name="lidea">
        <xs:annotation>
            <xs:documentation>Langer Gedankenstrich</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Langer Gedankenstrich ^+</xs:documentation>
            </xs:annotation>
        </xs:complexType>
    </xs:element>
    <xs:element name="euro">
        <xs:annotation>
            <xs:documentation>Zeichen "Euro"</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Euro Zeichen - zur einfacheren Verarbeitung (0x20AC)</xs:documentation>
            </xs:annotation>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="lt">
        <xs:annotation>
            <xs:documentation>Zeichen "kleiner als"</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Kleiner als: mathematisch</xs:documentation>
            </xs:annotation>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="gt">
        <xs:annotation>
            <xs:documentation>Zeichen "größer als"</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Größer als: mathematisch</xs:documentation>
            </xs:annotation>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="amp">
        <xs:annotation>
            <xs:documentation>Zeichen "Ampersand / Und"</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:annotation>
                <xs:documentation>Kaufmännisches "UND"</xs:documentation>
            </xs:annotation>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="bdquo">
        <xs:annotation>
            <xs:documentation>Typographisches Anführungszeichen unten ^0132 (HTML: #8222)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="ldquo">
        <xs:annotation>
            <xs:documentation>Typographisches Anführungszeichen oben ^0147 (HTML: #8220)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="em">
        <xs:annotation>
            <xs:documentation>Em-Abstand (HTML: &#8195;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="en">
        <xs:annotation>
            <xs:documentation>En-Abstand (HTML: &#8194;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="em14">
        <xs:annotation>
            <xs:documentation>1/4 Em-Abstand (HTML: &#8197;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="tm">
        <xs:annotation>
            <xs:documentation>Trademark-Zeichen (HTML: &#8482;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="dots">
        <xs:annotation>
            <xs:documentation>Auslassungszeichen (HTML: &#8230;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="rdquo">
        <xs:annotation>
            <xs:documentation>Schließendes doppeltes Anführungszeichen (HTML: #8221;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="sbquo">
        <xs:annotation>
            <xs:documentation>Typographisch einfach unten (HTML: &#8218;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="rsquo">
        <xs:annotation>
            <xs:documentation>Typographisch einfach oben, schließend (HTML: &#8217;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <xs:element name="lsquo">
        <xs:annotation>
            <xs:documentation>Typographisch einfach oben, öffnend (HTML: &#8216;)</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <!-- empty -->
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // Zusätzlicher Abstand zu Layoutzwecken      -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="abstand">
        <xs:annotation>
            <xs:documentation>09_Abstand</xs:documentation>
        </xs:annotation>
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0">
                <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
            </xs:choice>
            <xs:attribute name="halign"/>
            <xs:attribute name="ct" type="xs:string" use="optional"/>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- // PD-Parlamentsdirektion                                 -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="pdvorlage">
        <xs:complexType mixed="true">
            <xs:sequence>
                <xs:element ref="pdeinst"/>
                <xs:group ref="innerhalbAbsatz" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
            <xs:attribute name="ct" type="xs:string" use="optional"/>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="pdeinst">
        <xs:complexType mixed="true">
            <xs:sequence minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="tab"/>
            </xs:sequence>
            <xs:attribute name="formatname"/>
            <xs:attribute name="name"/>
            <xs:attribute name="size"/>
            <xs:attribute name="bold"/>
            <xs:attribute name="italic"/>
            <xs:attribute name="underline"/>
            <xs:attribute name="underlinecolor"/>
            <xs:attribute name="strikethrough"/>
            <xs:attribute name="doublestrikethrough"/>
            <xs:attribute name="outline"/>
            <xs:attribute name="emboss"/>
            <xs:attribute name="shadow"/>
            <xs:attribute name="hidden"/>
            <xs:attribute name="smallcaps"/>
            <xs:attribute name="allcaps"/>
            <xs:attribute name="color"/>
            <xs:attribute name="engrave"/>
            <xs:attribute name="superscript"/>
            <xs:attribute name="subscript"/>
            <xs:attribute name="scaling"/>
            <xs:attribute name="kerning"/>
            <xs:attribute name="animation"/>
            <xs:attribute name="leftindent"/>
            <xs:attribute name="rightindent"/>
            <xs:attribute name="spacebefore"/>
            <xs:attribute name="spacebeforeauto"/>
            <xs:attribute name="spaceafter"/>
            <xs:attribute name="spaceafterauto"/>
            <xs:attribute name="linespacingrule"/>
            <xs:attribute name="alignment"/>
            <xs:attribute name="widowcontrol"/>
            <xs:attribute name="keepwithnext"/>
            <xs:attribute name="keeptogether"/>
            <xs:attribute name="pagebreakbefore"/>
            <xs:attribute name="nolinenumber"/>
            <xs:attribute name="hyphenation"/>
            <xs:attribute name="firstlineindent"/>
            <xs:attribute name="outlinelevel"/>
            <xs:attribute name="characterunitleftindent"/>
            <xs:attribute name="characterunitrightindent"/>
            <xs:attribute name="characterunitfirstlineindent"/>
            <xs:attribute name="lineunitbefore"/>
            <xs:attribute name="lineunitafter"/>
        </xs:complexType>
    </xs:element>
    <!-- /////////////////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="div">
        <xs:annotation>
            <xs:documentation>generic language/style container</xs:documentation>
        </xs:annotation>
        <!-- seit 16.08.2005 -->
        <xs:complexType mixed="true">
            <xs:choice minOccurs="0" maxOccurs="unbounded">
                <xs:element ref="ueberschrift"/>
                <xs:element ref="absatz"/>
                <xs:element ref="ausgabe"/>
                <xs:element ref="abstand"/>
                <xs:element ref="liste"/>
                <xs:element ref="inhaltsvz"/>
                <xs:element ref="unterschrift"/>
                <xs:element ref="binary"/>
                <xs:element ref="textbox"/>
                <xs:element ref="beschr"/>
                <xs:element ref="table"/>
                <xs:element ref="pdvorlage"/>
            </xs:choice>
            <xs:attributeGroup ref="div.attlist"/>
        </xs:complexType>
    </xs:element>
    <xs:attributeGroup name="div.attlist">
        <xs:attribute name="id" type="xs:ID"/>
        <xs:attribute name="align" type="xs:string"/>
        <xs:attribute name="class" type="xs:string"/>
        <xs:attribute name="style" type="xs:string"/>
        <xs:attribute name="width" type="xs:string"/>
        <xs:attribute name="twips" type="xs:string"/>
    </xs:attributeGroup>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- Word: [Einfügen]-[Manueller Wechsel...] -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="layoutdaten">
        <xs:annotation>
            <xs:documentation>Word Dokumenteinstellungen (Properties). Alle Angaben in pt (Punkt).</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence minOccurs="0">
                <xs:element name="classes" type="xs:string"/>
            </xs:sequence>
            <xs:attribute name="paperf"/>
            <xs:attribute name="paperw"/>
            <xs:attribute name="paperh"/>
            <xs:attribute name="margl"/>
            <xs:attribute name="margr"/>
            <xs:attribute name="margt"/>
            <xs:attribute name="margb"/>
            <xs:attribute name="headerd"/>
            <xs:attribute name="footerd"/>
            <xs:attribute name="gutter"/>
            <xs:attribute name="gutterpos"/>
            <xs:attribute name="deftab"/>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:element name="wechsel">
        <xs:annotation>
            <xs:documentation>Wechsel</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:attribute name="typ">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="s">
                            <xs:annotation>
                                <xs:documentation>manueller Seitenwechsel: ^m</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                        <xs:enumeration value="sp">
                            <xs:annotation>
                                <xs:documentation>manueller Spaltenwechsel: ^n</xs:documentation>
                            </xs:annotation>
                        </xs:enumeration>
                    </xs:restriction>
                </xs:simpleType>
            </xs:attribute>
        </xs:complexType>
    </xs:element>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <!-- RIS-Erweiterung für die Blockung von Content -->
    <!-- //////////////////////////////////////////////////////////////////////////// -->
    <xs:complexType name="blockType">
        <xs:choice minOccurs="0" maxOccurs="unbounded">
            <xs:element ref="ueberschrift"/>
            <xs:element ref="absatz"/>
            <xs:element ref="ausgabe"/>
            <xs:element ref="abstand"/>
            <xs:element ref="liste"/>
            <xs:element ref="inhaltsvz"/>
            <xs:element ref="unterschrift"/>
            <xs:element ref="binary"/>
            <xs:element ref="textbox"/>
            <xs:element ref="beschr"/>
            <xs:element ref="table"/>
            <xs:element ref="pre"/>
            <xs:element ref="br"/>
            <xs:element ref="nbsp"/>
            <xs:element ref="pdvorlage"/>
            <xs:element ref="div"/>
            <xs:group ref="zeichenformate" minOccurs="0" maxOccurs="unbounded"/>
        </xs:choice>
        <xs:attribute name="name">
            <xs:annotation>
                <xs:documentation>Bezeichnung des Blocks (vgl. Kategorie im CPL)</xs:documentation>
            </xs:annotation>
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="betreff"/>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <!-- //////////////////////////////////////////////////////////////////////////// -->
</xs:schema>
